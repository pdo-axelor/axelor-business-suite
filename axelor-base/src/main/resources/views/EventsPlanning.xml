<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_4.1.xsd">

	<grid name="events-planning-grid" title="Events plannings" model="com.axelor.apps.base.db.EventsPlanning">
		<field name="name"/>
	</grid>
	
	<form name="events-planning-form" title="Events planning" model="com.axelor.apps.base.db.EventsPlanning">
		<panel name="main" >
			<field name="name"/>
		</panel>
        <panel-related field="eventsPlanningLineList" editable="true" grid-view="events-planning-line-grid" form-view="events-planning-line-form"/>
	</form>

</object-views>