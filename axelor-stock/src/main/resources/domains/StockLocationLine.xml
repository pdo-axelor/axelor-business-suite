<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">

  <module name="stock" package="com.axelor.apps.stock.db"/>

  <entity name="StockLocationLine" lang="java">
  
  	<many-to-one name="stockLocation" ref="com.axelor.apps.stock.db.StockLocation" title="Stock location"/>
  	
	<many-to-one name="product" ref="com.axelor.apps.base.db.Product" title="Product"/>    
	<decimal name="currentQty" title="Current Qty"/>
	<decimal name="futureQty" title="Future Qty"/>
	<decimal name="avgPrice" precision="20" scale="10" readonly="true" title="Average Price"/>
	<date name="lastFutureStockMoveDate" title="Last Future Stock Move"/>
	
    <string name="rack" title="Rack"/>
	<many-to-one name="trackingNumber" ref="com.axelor.apps.stock.db.TrackingNumber" title="Tracking Nbr"/>
	<many-to-one name="detailsLocation" ref="com.axelor.apps.stock.db.StockLocation" title="Location Details"/>
	
	<decimal name="lastInventoryRealQty"/>
	<datetime name="lastInventoryDateT" tz="true" title="Last inventory date"/>
	
  </entity>
</domain-models>
